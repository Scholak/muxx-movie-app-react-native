generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Subscription {
  MONTHLY
  YEARLY
}

model User {
  id           Int          @id @default(autoincrement())
  email        String       @unique
  password     String
  subscription Subscription
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
}
